name: gitea
services:
  gitea:
    cpu_shares: 90
    command: []
    container_name: gitea
    deploy:
      resources:
        limits:
          memory: 256M
        reservations:
          memory: "268435456"
    environment:
      - USER_GID=1000
      - USER_UID=1000
    hostname: gitea
    image: gitea/gitea:latest
    labels:
      icon: https://cdn.jsdelivr.net/gh/IceWhaleTech/CasaOS-AppStore@main/Apps/Gitea/icon.png
    ports:
      - target: 3000
        published: "3002"
        protocol: tcp
      - target: 22
        published: "3264"
        protocol: tcp
    restart: unless-stopped
    volumes:
      - type: bind
        source: /DATA/AppData/gitea/data
        target: /data
    x-casaos:
      envs:
        - container: USER_UID
          description:
            en_us: The UID (Unix user ID) of the user that runs Gitea
            zh_cn: Gitea 运行的用户的 UID（Unix 用户 ID）
        - container: USER_GID
          description:
            en_us: The GID (Unix group ID) of the user that runs Gitea
            zh_cn: Gitea 运行的用户的 GID（Unix 组 ID）
      ports:
        - container: "3000"
          description:
            en_us: WebUI Port
            zh_cn: WebUI 端口
        - container: "22"
          description:
            en_us: SSH Port
            zh_cn: SSH 端口
      volumes:
        - container: /data
          description:
            en_us: Gitea data directory.
            zh_cn: Gitea 数据目录。
    devices: []
    cap_add: []
    network_mode: bridge
    privileged: false
  gitea_act_runner:
    environment:
      - GITEA_INSTANCE_URL=https://git.juhdi.my.id
      - GITEA_RUNNER_REGISTRATION_TOKEN=9vYEC5Q7MPUyLsDxuvMXN5iwSlLMNLSH71hiXPdZ
      - TZ=Europe/Berlin
    image: vegardit/gitea-act-runner:dind-latest
    privileged: true
    restart: always
    volumes:
      - type: bind
        source: /DATA/AppData/gitea/data
        target: /data
        bind:
          create_host_path: true
    ports: []
    devices: []
    cap_add: []
    command: []
    networks:
      - default
    container_name: ""
    hostname: ""
    cpu_shares: 90
    deploy:
      resources:
        limits:
          memory: 256M
networks:
  default:
    name: gitea_default
